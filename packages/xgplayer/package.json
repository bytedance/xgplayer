{
  "name": "xgplayer",
  "version": "3.0.0-rc.2",
  "description": "video player",
  "main": "dist/index.min.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "bin": {
    "xgplayer": "bin/xgplayer.js"
  },
  "scripts": {
    "postpublish": "git push --tags",
    "release:alpha": "npm version prerelease --preid=alpha && npm publish --tag alpha --registry=http://registry.npmjs.org",
    "release:alpha-test": "npm publish --tag alpha-test --registry=http://registry.npmjs.org",
    "release:rc": "npm version prerelease --preid=rc && npm publish --tag rc --registry=http://registry.npmjs.org",
    "prepare": "npm run build",
    "tsc:umd": "tsc --project ./tsconfig.umd.json",
    "tsc:es": "tsc --outDir es",
    "build": "npm run build:es && npm run build:main && npm run build:style",
    "build:main": "rm -rf ./dist && cross-env NODE_ENV=production rollup -c rollup.config.js && npm run tsc:umd",
    "build:browser": "rm -rf ./browser && node scripts/buildBrowser.js",
    "build:es": "rm -rf ./es && ../../node_modules/.bin/babel src --out-dir es && npm run tsc:es",
    "build:lib": "rm -rf ./lib && npx cross-env BABEL_TARGET=commonjs ../../node_modules/.bin/babel ./src --out-dir lib",
    "build:style": "node scripts/buildStyles.js",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "unpkgFiles": [
    "browser",
    "dist"
  ],
  "keywords": [
    "video",
    "player"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/xgplayer.git"
  },
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "browserslist": [
    "> 5%",
    "IE 9",
    "iOS 7",
    "Firefox > 20"
  ],
  "dependencies": {
    "danmu.js": "^0.5.4",
    "deepmerge": "^1.5.0",
    "delegate": "^3.2.0",
    "downloadjs": "1.4.7",
    "eventemitter3": "^4.0.7"
  },
  "devDependencies": {},
  "peerDependencies": {
    "danmu.js": "^0.5.0",
    "xgplayer-subtitles": "^1.0.9-alpha.1"
  }
}
