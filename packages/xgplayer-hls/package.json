{
  "name": "xgplayer-hls",
  "version": "2.2.0-alpha.17",
  "description": "xgplayer plugin for hls",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "webpack --progress --display-chunks -p",
    "development": "webpack --progress --display-chunks --config webpack.config.dev.js",
    "watch": "webpack --config webpack.config.dev.js --watch --progress",
    "dev": "webpack --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "keywords": [
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "yinguohui@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "xgplayer-utils": "^2.2.0-alpha.0",
    "xgplayer-hls-live": "^2.2.0-alpha.0",
    "xgplayer-hls-vod": "^2.2.0-alpha.0",
    "concat-typed-array": "^1.0.2",
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5"
  },
  "peerDependencies": {
    "xgplayer": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2"
  }
}
