{
  "name": "xgplayer-dash",
  "version": "1.1.4-beta.2",
  "description": "xgplayer plugin for MPEG-DASH",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "webpack --progress --display-chunks -p",
    "watch": "webpack --progress --display-chunks -p --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bytedance/xgplayer.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "babel-plugin-bulk-import"
    ]
  },
  "keywords": [
    "MPEG-DASH",
    "mp4",
    "fmp4",
    "player",
    "video"
  ],
  "author": "zhangxin.frontend@bytedance.com",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^2.0.1",
    "event-emitter": "^0.3.5",
    "xml2js": "^0.4.19"
  },
  "peerDependency": {
    "xgplayer": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-bulk-import": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "json-loader": "^0.5.7",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.1.2"
  }
}
