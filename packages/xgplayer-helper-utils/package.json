{
  "name": "xgplayer-helper-utils",
  "version": "2.4.22",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run build",
    "watch": "npm run watch:es & npm run watch:lib",
    "watch:es": "babel src --out-dir es --watch",
    "watch:lib": "babel src --out-dir lib --presets=es2015 --watch & tsc --watch",
    "build": "npm run build:es && npm run build:lib",
    "build:es": "babel src --out-dir es && tsc",
    "build:lib": "babel src --out-dir lib --presets=es2015 && tsc --module CommonJS --outDir lib"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ]
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "xgplayer-helper-models": "2.4.22"
  }
}
